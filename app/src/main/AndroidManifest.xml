<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.d3ifcool.zeitplannew">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="org.d3ifcool.zeitplannew.App"
        android:allowBackup="true"
        android:icon="@mipmap/z_logo_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.d3ifcool.zeitplannew.AddActivity"
            android:parentActivityName="org.d3ifcool.zeitplannew.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.d3ifcool.zeitplannew.MainActivity"
                />
        </activity>
        <activity
            android:name="org.d3ifcool.zeitplannew.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:authorities="org.d3ifcool.zeitplannew"
            android:name="org.d3ifcool.zeitplannew.data.JadwalProvider"
            android:exported="false"/>

        <service android:name="org.d3ifcool.zeitplannew.reminder.ReminderAlarmService"
            android:exported="false"/>

    </application>

</manifest>